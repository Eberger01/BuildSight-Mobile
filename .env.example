# Gemini AI Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
# Get these from your Supabase project settings
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Backend Mode
# Set to 'true' to use Supabase Edge Functions for AI calls (secure, credit-based)
# Set to 'false' to use direct Gemini API calls (development mode)
EXPO_PUBLIC_USE_BACKEND=false

# RevenueCat Configuration
# Get these from https://app.revenuecat.com after creating your project
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your_revenuecat_ios_api_key
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your_revenuecat_android_api_key